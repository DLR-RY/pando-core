<pando
  version="1.2.0"
  xmlns:xi="http://www.w3.org/2001/XInclude"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
  xsd:noNamespaceSchemaLocation="http://www.dlr.de/schema/pando/pando.xsd">

  <service>
    <parameters>
      <parameter name="Parameter 1" uid="p1" type="uint8" />
      
      <list name="List 1" uid="list1">
        <parameterRef uid="p1" />
        <parameter name="Parameter 2" uid="p2" type="uint16" />
        <parameter name="Parameter 3" uid="p3" type="uint32" />
      </list>
    
      <list name="List 2" uid="list2">
        <parameterRef uid="p1" />
        <parameterRef uid="list1" />
        <parameter name="Parameter 3" uid="p4" type="uint32" />
      </list>
    </parameters>
    
    <telecommands>
      <telecommand name="Test" uid="test">
        <serviceType>0</serviceType>
        <serviceSubtype>0</serviceSubtype>
        
        <parameters>
          <parameterRef uid="list2" />
        </parameters>
      </telecommand>
    </telecommands>
  </service>
</pando>
